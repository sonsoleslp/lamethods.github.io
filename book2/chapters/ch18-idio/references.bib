@article{Respondek_Seufert_Nett_2019,
	title        = {Adding previous experiences to the person-situation debate of achievement emotions},
	author       = {Respondek, Lisa and Seufert, Tina and Nett, Ulrike E.},
	year         = 2019,
	month        = {Jul},
	journal      = {Contemporary educational psychology},
