@article{Malmberg_Saqr_Järvenoja_Järvelä_2022,
	title        = {How the monitoring events of individual students are associated with phases of regulation: A network analysis approach},
	author       = {Malmberg, Jonna and Saqr, Mohammed and Järvenoja, Hanna and Järvelä, Sanna},
